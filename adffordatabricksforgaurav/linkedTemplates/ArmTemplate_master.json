{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adffordatabricksforgaurav"
		},
		"linked_service_azuredatabricks_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'linked_service_azuredatabricks_datalake'"
		},
		"linked_service_database_azureproject_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'linked_service_database_azureproject'"
		},
		"linked_service_azuredatabricks_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://safordatabricksforgaurav.dfs.core.windows.net/"
		},
		"linked_service_database_azureproject_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyserverdatabase.database.windows.net"
		},
		"linked_service_database_azureproject_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifydatabase "
		},
		"linked_service_database_azureproject_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "gauravsoni"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adffordatabricksforgaurav_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"linked_service_azuredatabricks_datalake_accountKey": {
						"value": "[parameters('linked_service_azuredatabricks_datalake_accountKey')]"
					},
					"linked_service_database_azureproject_password": {
						"value": "[parameters('linked_service_database_azureproject_password')]"
					},
					"linked_service_azuredatabricks_datalake_properties_typeProperties_url": {
						"value": "[parameters('linked_service_azuredatabricks_datalake_properties_typeProperties_url')]"
					},
					"linked_service_database_azureproject_properties_typeProperties_server": {
						"value": "[parameters('linked_service_database_azureproject_properties_typeProperties_server')]"
					},
					"linked_service_database_azureproject_properties_typeProperties_database": {
						"value": "[parameters('linked_service_database_azureproject_properties_typeProperties_database')]"
					},
					"linked_service_database_azureproject_properties_typeProperties_userName": {
						"value": "[parameters('linked_service_database_azureproject_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}