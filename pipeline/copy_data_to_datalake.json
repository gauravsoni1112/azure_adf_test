{
	"name": "copy_data_to_datalake",
	"properties": {
		"activities": [
			{
				"name": "FetchLastCDC",
				"type": "Lookup",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "JsonSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "JsonReadSettings"
						}
					},
					"dataset": {
						"referenceName": "readDynamicJson",
						"type": "DatasetReference",
						"parameters": {
							"container": {
								"value": "@pipeline().parameters.container",
								"type": "Expression"
							},
							"folder": {
								"value": "@concat(pipeline().parameters.table_name,'/',pipeline().parameters.cdc_folder)",
								"type": "Expression"
							},
							"file": {
								"value": "@pipeline().parameters.cdc_file",
								"type": "Expression"
							}
						}
					},
					"firstRowOnly": false
				}
			}
		],
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "bronze"
			},
			"table_name": {
				"type": "string"
			},
			"cdc_folder": {
				"type": "string",
				"defaultValue": "cdc"
			},
			"cdc_file": {
				"type": "string",
				"defaultValue": "cdc.json"
			}
		},
		"annotations": []
	}
}